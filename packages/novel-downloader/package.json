    "do-init": "npm run check-updates && npm run do-install",
    "do-install": "npx yarn-tool install",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "prepublishOnly": "npm run check-updates",
